@page
@model _1.Semester_Projekt.Pages.CRUD.EditLydModel
@{
}

<h1>Edit Lyd</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Fact.Id" class="control-label"></label>
        <input asp-for="@Model.Fact.Id" class="form-control" type="number" />
        <span asp-validation-for="@Model.Fact.Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Fact.EmneTekst" class="control-label"></label>
        <input asp-for="@Model.Fact.EmneTekst" class="form-control" type="text" />
        <span asp-validation-for="@Model.Fact.EmneTekst" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Edit" class="btn btn-primary" />
    </div>
</form>
