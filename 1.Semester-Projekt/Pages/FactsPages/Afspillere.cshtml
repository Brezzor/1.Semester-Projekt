@page
@model AfspillereModel
@{
    ViewData["Title"] = "Dans";
}
<h1>Dans</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Navn
            </th>
            <th>
                Info
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Faktas)
        {
            <tr>
                <td>
                    @item.Value.Id
                </td>
                <td>
                    @item.Value.Header
                </td>
                <td>
                    @item.Value.Body
                </td>
            </tr>}
    </tbody>

</table>