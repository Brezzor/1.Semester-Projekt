@page
@model MapModel
@{
    ViewData["Title"] = "Map Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome To @ViewData["Title"]</h1>
</div>
<button type ="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#MapModal"> Open map</button>
<div id="MapModal" class=" modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"> &times;</button>
                <h4 class="modal-title"> Map 1</h4>
            </div>
            <div class="modal-body">
                <img src="~/images/Rockmuseet 1 sal.jpg" style="width:1280px;height:720px;" />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>